<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Indicator2.GerenciarIndie"
             BackgroundColor="#23272A">
    <ContentPage.Content>
        <StackLayout>


            <StackLayout Padding="12,30,0,50" VerticalOptions="Start" HorizontalOptions="Start">
                <Label Text="Administrar" TextColor="#ffe600" FontSize="30" FontFamily="Poppins"/>
                <Button x:Name="BTNApagar" Padding="70,0,70,0" Margin="0,40,0,0" Text="Gerenciar" FontFamily="Poppins-Bold" BackgroundColor="#ffe600" TextColor="Black" HorizontalOptions="Center"
                  CornerRadius="20" Clicked="BTNApagar_Clicked"
                />

            </StackLayout>
        
        
        </StackLayout>
    </ContentPage.Content>
</ContentPage>